{
    "module_name": "single3V2",
    "channels": [
        "ch1",
        "ch2",
        "ch3",
        "ch4"
    ],
    "recursive_variables": {
        "sw": "(ch1 ? \"one\") ; sw1 o+ (ch2 ? \"one\") ; sw2 o+ (ch3 ? \"one\") ; sw3 o+ (ch4 ? \"one\") ; sw4",
        "h2": "(ch4 ! \"one\") ; h2",
        "h3": "(ch1 ! \"one\") ; h3 o+ (ch3 ! \"one\") ; h3",
        "h1": "(ch2 ! \"one\") ; h1",
        "sw1": "\"src = 3 . dst = 1\" ; (\"srcaddr <- 10003 . dstaddr <- 10001\" ; sw1)",
        "sw2": "\"src = 1 . dst = 3\" ; (\"srcaddr <- 10001 . dstaddr <- 10003\" ; sw2)",
        "sw3": "\"src = 3 . dst = 2\" ; (\"srcaddr <- 10003 . dstaddr <- 10002\" ; sw3)",
        "sw4": "\"src = 2 . dst = 3\" ; (\"srcaddr <- 10002 . dstaddr <- 10003\" ; sw4)"
    },
    "program": "sw || h2 || h3 || h1",
    "in_packets": {
        "first": "src = 1 . dst = 3"
    },
    "out_packets": {
        "first": "srcaddr = 10001 . dstaddr = 10003"
    },
    "properties": {
        "first": [
            [
                "r",
                "(head(tail(@Program, {rcfg(ch2, \"one\")})))",
                "!0",
                2
            ]
        ]
    },
    "file_name": "single3V2.maude",
    "comm": [
        "(ch2,\"one\")",
        "(ch4,\"one\")",
        "(ch3, \"one\")",
        "(ch3,\"one\")",
        "(ch1,\"one\")"
    ]
}